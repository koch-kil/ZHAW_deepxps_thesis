	urldate = {2018-07-10},
	journal = {Asian Pacific Journal of Tropical Medicine},
	author = {DebMandal, Manisha and Mandal, Shyamapada},
	month = mar,
	year = {2011},
	keywords = {Coconut, Disease prevention, Medicinal property, Nutritional value},
	pages = {241--247},
}

