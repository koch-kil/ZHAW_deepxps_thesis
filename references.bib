	journal = {Journal of Food Engineering},
	author = {Matsuura, Tsutashi and Ogawa, Akihiro and Tomabechi, Masaki and Matsushita, Ryo and Gohtani, Shoichi and Neoh, Tze Loon and Yoshii, Hidefumi},
	month = oct,
	year = {2015},
	keywords = {Emulsion, Interaction, Spray drying, Sugar fatty acid esters},
