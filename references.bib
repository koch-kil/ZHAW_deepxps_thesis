@article{wolf_glycemic_2001,
	title = {Glycemic {Response} to a {Food} {Starch} {Esterified} by 1-{Octenyl} {Succinic} {Anhydride} in {Humans}},
	volume = {49},
	issn = {0021-8561},
	url = {https://doi.org/10.1021/jf0015017},
	doi = {10.1021/jf0015017},
	abstract = {To evaluate the glycemic response to a food starch esterified by 1-octenyl succinic anhydride (OSA), 30 healthy nondiabetic adult subjects were studied in a double-blind crossover design. After an overnight fast, subjects consumed a product containing either 25 g of glucose or 25 g of OSA-substituted starch. Finger-prick capillary blood was obtained at baseline and 15, 30, 45, 60, 90, and 120 min postprandial for glucose measurement. After OSA treatment, the rise in blood glucose was reduced (P {\textless} 0.05) at 15 and 30 min and tended (P {\textless} 0.08) to be lower at 45 min. Mean peak rise in glucose was reduced 19\% (P {\textless} 0.01) by OSA (3.30 ± 0.19 versus 2.66 ± 0.16 mmol/L) compared to glucose, but time to peak did not differ between treatments. Net incremental area under the curve was also lower (P {\textless} 0.05) on OSA compared to glucose. Minimal effects on gastrointestinal symptoms (intensity and frequency of nausea, cramping, distention, and flatulence) were noted for both products, with no clinically significant difference between products. In conclusion, starch substitution with OSA attenuated the postprandial glycemic excursion compared to an equivalent glucose challenge and was well tolerated by fasting healthy adult subjects. Keywords: Glycemic response; humans; 1-octenyl succinic anhydride; modified starch},
	number = {5},
	urldate = {2018-08-02},
	journal = {Journal of Agricultural and Food Chemistry},
	author = {Wolf, Bryan W. and Wolever, Thomas M. S. and Bolognesi, Claudia and Zinker, Bradley A. and Garleb, Keith A. and Firkins, Jeffrey L.},
	month = may,
	year = {2001},
	pages = {2674--2678},
}

