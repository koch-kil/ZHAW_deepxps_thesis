	journal = {Nutrition},
	author = {Courchesne-Loyer, Alexandre and St-Pierre, Valérie and Hennebelle, Marie and Castellano, Christian-Alexandre and Fortier, Mélanie and Tessier, Daniel and Cunnane, Stephen C.},
	month = oct,
	year = {2015},
	keywords = {Acetoacetate, Fibrate, Ketones, Lipid metabolism, Medium-chain triacylglycerols, β-hydroxybutyrate},
	pages = {1255--1259},
}

