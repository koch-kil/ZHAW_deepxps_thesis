	author = {Sharif, Hafiz Rizwan and Goff, H. Douglas and Majeed, Hamid and Shamoon, Muhammad and Liu, Fei and Nsor-Atindana, John and Haider, Junaid and Liang, Rong and Zhong, Fang},
