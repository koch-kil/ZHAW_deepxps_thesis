@article{karupaiah_effects_2007,
	title = {Effects of stereospecific positioning of fatty acids in triacylglycerol structures in native and randomized fats: a review of their nutritional implications},
	volume = {4},
	issn = {1743-7075},
	shorttitle = {Effects of stereospecific positioning of fatty acids in triacylglycerol structures in native and randomized fats},
